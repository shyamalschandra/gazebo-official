      </taskGroup>
      
      <taskGroup name="Pinch">
        <task id="pinch_1" icon="file://media/gui/arat/arat_icons/pinch_1.jpg" name="Ball bearing" instructions="Pick up the ball bearing and place it on top of the box, inside of the tin lid."></task>
        <task id="pinch_2" icon="file://media/gui/arat/arat_icons/pinch_2.jpg" name="Marble" instructions="Pick up the marble and place it on top of the box, inside of the tin lid."></task>
