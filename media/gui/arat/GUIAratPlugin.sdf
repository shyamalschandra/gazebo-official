        <task id="grasp_2" icon="file://media/gui/arat/arat_icons/grasp_2.jpg" name="Block 2.5cm" instructions="Pick up the 2.5 cm block and place it on top of the box."></task>
        <task id="grasp_3" icon="file://media/gui/arat/arat_icons/grasp_3.jpg" name="Block 5cm" instructions="Pick up the 5 cm block and place it on top of the box."></task>
        <task id="grasp_4" icon="file://media/gui/arat/arat_icons/grasp_4.jpg" name="Block 7.5cm" instructions="Pick up the 7.5 cm block and place it on top of the box."></task>
        <task id="grasp_5" icon="file://media/gui/arat/arat_icons/grasp_5.jpg" name="Cricket ball" instructions="Pick up the cricket ball and place it on top of the box, inside of the tin lid."></task>
        <task id="grasp_6" icon="file://media/gui/arat/arat_icons/grasp_6.jpg" name="Stone" instructions="Pick up the stone and place it on top of the box."></task>
