<?xml version="1.0" ?>
<sdf version="1.4">
    <world name="default">
        <scene>
            <ambient>1.0 1.0 1.0 1</ambient>
            <background>0 0 0 1.0</background>
            <shadows>true</shadows>
        </scene>
        <physics type="rtql8">
            <gravity>0 0 -10.0</gravity>
        </physics>
        <!--include><uri>model://ground_plane</uri></include-->
        <!-- 25.3kg total mass -->
        <model name="test_model">
            <pose>0 0 4 0 0 0</pose>
            <link name="link1">
                <pose>0 0 -2 0 0 0</pose>
                <inertial>
                    <inertia>
                        <ixx>1</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>1</iyy>
                        <iyz>0</iyz>
                        <izz>1</izz>
                    </inertia>
                    <mass>1.0</mass>
                </inertial>
                <visual name="visual">
                    <pose>0 0 0 0 0 0</pose>
                    <geometry>
                        <box>
                            <size>0.2 0.4 0.6</size>
                        </box>
                    </geometry>
                    <material>
                        <script>Gazebo/WoodPallet</script>
                    </material>
                </visual>
            </link>
            <joint name="joint01" type="revolute">
                <pose>0 -0.5 0 0 0 0</pose>
                <axis>
