    <joint:hinge name="left_wheel_hinge">
      <body1>left_wheel</body1>
      <body2>chassis</body2>
      <anchor>left_wheel</anchor>
      <axis>0 1 0</axis>
      <erp>0.8</erp>
      <cfm>10e-5</cfm>
    </joint:hinge>

    <joint:hinge name="right_wheel_hinge">
      <body1>right_wheel</body1>
      <body2>chassis</body2>
      <anchor>right_wheel</anchor>
      <axis>0 1 0</axis>
      <erp>0.8</erp>
      <cfm>10e-5</cfm>
    </joint:hinge>

    <joint:ball name="front_castor_joint">
      <body1>front_castor</body1>
      <body2>chassis</body2>
      <anchor>front_castor</anchor>
      <erp>0.8</erp>
      <cfm>10e-5</cfm>
    </joint:ball>

    <joint:hinge name="rear_castor_joint">
      <body1>rear_castor</body1>
      <body2>chassis</body2>
      <anchor>rear_castor</anchor>
      <axis>0 1 0</axis>
      <erp>0.8</erp>
      <cfm>10e-5</cfm>
    </joint:hinge>

    <controller:differential_position2d name="controller1">
      <leftJoint>left_wheel_hinge</leftJoint>
      <rightJoint>right_wheel_hinge</rightJoint>
      <wheelSeparation>0.260</wheelSeparation>
      <wheelDiameter>0.066</wheelDiameter>
      <torque>1</torque>
      <interface:position name="position_iface_0"/>
    </controller:differential_position2d>
</model:physical>
