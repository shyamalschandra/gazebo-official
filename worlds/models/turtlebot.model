    <body:cylinder name="left_wheel">
      <rpy>0 90 90</rpy>
      <xyz>0 0.130 0.033</xyz>
      <geom:cylinder name="geom">
        <mass>0.1</mass>
        <size>0.033 0.023</size>
        <visual>
          <size>0.066 0.066 0.023</size>
          <mesh>unit_cylinder</mesh>
          <material>Gazebo/Black</material>
        </visual>
      </geom:cylinder>

